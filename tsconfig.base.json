{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["es2020"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "target": "es2020",
    "skipLibCheck": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "composite": true,
    "paths": {
      "@acala-network/sdk": [ "packages/sdk/src"],
      "@acala-network/sdk/*": ["packages/sdk/src/*"],
      "@acala-network/sdk-core": ["packages/sdk-core/src"],
      "@acala-network/sdk-core/*": ["packages/sdk-core/src/*"],
      "@acala-network/sdk-loan": ["packages/sdk-loan/src"],
      "@acala-network/sdk-loan/*": ["packages/sdk-loan/src/*"],
      "@acala-network/sdk-swap": ["packages/sdk-swap/src"],
      "@acala-network/sdk-swap/*": ["packages/sdk-swap/src/*"],
      "@acala-network/sdk-wallet": ["packages/sdk-wallet/src"],
      "@acala-network/sdk-wallet/*": ["packages/sdk-wallet/src/*"],
      "@acala-network/wormhole-portal": ["packages/wormhole-portal/src"],
      "@acala-network/wormhole-portal/*": ["packages/wormhole-portal/src/*"],
      "@polkadot/types/lookup": ["node_modules/@acala-network/types/lookup/types-acala.d.ts"],
    },
  },
}
