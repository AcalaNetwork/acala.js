{
  "name": "@acala-network/sdk",
  "version": "4.1.10-1",
  "description": "acala network sdk",
  "author": "Acala Developers <hello@acala.network>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "contract:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "swc ./src --config-file ../../.cjsswcrc -d dist/cjs --copy-files && tsc -p tsconfig.json --declarationDir dist/cjs && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "swc ./src --config-file ../../.esmswcrc -d dist/esm --copy-files && tsc -p tsconfig.json --declarationDir dist/esm"
  },
  "type": "module",
  "files": [
    "dist/cjs/**",
    "dist/esm/**"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "repository": "https://github.com/AcalaNetwork/acala.js.git",
  "bugs": {
    "url": "https://github.com/AcalaNetwork/acala.js/issues"
  },
  "homepage": "https://github.com/AcalaNetwork/acala.js",
  "peerDependencies": {
    "@acala-network/api": "^5",
    "@acala-network/eth-providers": "~2.7.18",
    "@polkadot/api": "^10",
    "ethers": "~5.7.0"
  },
  "dependencies": {
    "@acala-network/api": "^5.1.1",
    "@acala-network/eth-providers": "~2.7.18",
    "@acala-network/sdk-core": "workspace:*",
    "@polkadot/api": "^10.9.1",
    "axios": "^0.24.0",
    "ethers": "~5.7.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "lodash": "^4.17.20",
    "lru-cache": "^7.14.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.161",
    "@types/lru-cache": "^7.10.10",
    "typechain": "^8.1.0"
  }
}
